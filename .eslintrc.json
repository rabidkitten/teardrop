{
  "env": {
      "browser": true,
      "es2021": true,
      "jasmine": true,
      "jest": true
  },
  "extends": [
      "plugin:react/recommended",
      "airbnb",
      "plugin:jsdoc/recommended",
      "plugin:storybook/recommended"
  ],
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "ignorePatterns": ["node_modules/*"],
  "plugins": [
      "react",
      "jsdoc"
  ],
  "rules": {
      "no-restricted-exports": "off",
      "react/require-default-props": "off"
  }
}
